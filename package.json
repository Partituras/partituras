{
  "name": "partituras",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "start:partitura-microservice": "nx run partitura-microservice:serve",
    "start:search-microservice": "nx run search-microservice:serve",
    "start:graphql-api": "nx run graphql-api:serve",
    "start:docker-compose": "docker-compose up",
    "build": "nx build",
    "test": "nx test",
    "build:partitura-microservice": "nx run partitura-microservice:build:production && docker build --build-arg MICROSERVICE=partitura-microservice -t partituras/partitura:latest .",
    "build:search-microservice": "nx run search-microservice:build:production && docker build --build-arg MICROSERVICE=search-microservice -t partituras/search:latest .",
    "build:graphql-api": "nx run graphql-api:build:production && docker build --build-arg MICROSERVICE=graphql-api -t partituras/graphql:latest ."
  },
  "private": true,
  "devDependencies": {
    "@nestjs/schematics": "^8.0.4",
    "@nestjs/testing": "^8.1.1",
    "@nrwl/cli": "12.10.0",
    "@nrwl/eslint-plugin-nx": "12.10.0",
    "@nrwl/jest": "12.10.0",
    "@nrwl/linter": "12.10.0",
    "@nrwl/nest": "^12.10.0",
    "@nrwl/next": "^12.10.0",
    "@nrwl/node": "^12.10.0",
    "@nrwl/tao": "12.10.0",
    "@nrwl/workspace": "12.10.0",
    "@types/jest": "27.0.2",
    "@types/node": "16.11.1",
    "@typescript-eslint/eslint-plugin": "~5.1.0",
    "@typescript-eslint/parser": "~5.1.0",
    "eslint": "8.0.1",
    "eslint-config-prettier": "8.3.0",
    "jest": "27.3.1",
    "prettier": "^2.4.1",
    "ts-jest": "27.0.7",
    "typescript": "~4.4.4"
  },
  "dependencies": {
    "@grpc/grpc-js": "^1.4.1",
    "@grpc/proto-loader": "^0.6.6",
    "@nestjs/common": "^8.1.1",
    "@nestjs/core": "^8.1.1",
    "@nestjs/graphql": "^9.0.6",
    "@nestjs/microservices": "^8.1.1",
    "@nestjs/mongoose": "^9.0.0",
    "@nestjs/platform-express": "^8.1.1",
    "apollo-server-express": "^3.4.0",
    "graphql": "^15.6.1",
    "reflect-metadata": "^0.1.13",
    "rxjs": "~7.4.0",
    "tslib": "^2.3.1"
  }
}
